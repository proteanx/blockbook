{
  "coin": {
    "name": "DeVault",
    "shortcut": "DVT",
    "label": "DeVault",
    "alias": "devault"
  },
  "ports": {
    "backend_rpc": 8089,
    "backend_message_queue": 38389,
    "blockbook_internal": 9089,
    "blockbook_public": 9189
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpc",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://127.0.0.1:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-devault",
    "package_revision": "satoshilabs-1",
    "system_user": "devault",
    "version": "1.0.1",
    "binary_url": "https://github.com/devaultcrypto/devault/releases/download/v1.0.1/devault-1.0.1-x86_64-linux-gnu.tar.gz",
    "verification_type": "sha256",
    "verification_source": "17d5b90c135be5c5a99978511da8eac278d890344f50ef7cea2e12a0a882cd4b",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
      "bin/devault-qt"
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/devaultd -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "devault_like.conf",
    "client_config_file": "devault_like_client.conf"
  },
  "blockbook": {
    "package_name": "blockbook-devault",
    "system_user": "blockbook-devault",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "",
    "block_chain": {
      "parse": true,
      "subversion": "/DeVault Core:1.0.1/",
      "address_format": "cashaddr",
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "xpub_magic": 76067358,
      "slip44": 339,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "Proteus",
    "package_maintainer_email": "pro@proteanx.com"
  }
}
